<統計社会学研究アプリ:body>
	<決定木>
		<決定木メタデータ>
			<次元 名前="性別" 型="文字" 範囲="女 男" />
			<次元 名前="家族人数" 型="数字" 範囲="0 5" />
			<次元 名前="生存" 型="数字" 範囲="0 1" />
			<葉 次元="生存" 実際の範囲="0.1 0.9"/>
		</決定木メタデータ>
		<根>
			<分岐 次元="性別">
				<枝 範囲="女 女">
					<分岐 次元="家族人数" 型="数字" 範囲="0 5">
						<次元 範囲="0 2">
							<葉 範囲="0.6 0.9" />
						</次元>
						<次元 範囲="2 5">
							<葉 範囲="0.5 0.6" />
						</次元>
					</分岐>

				</枝>
				<枝 範囲="男 男">
					<葉 範囲="0.1 0.4" />
				</枝>
			</分岐>
		</根>
	</決定木>
